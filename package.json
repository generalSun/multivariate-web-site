{
  "name": "multivariate-web-site",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --mode development",
    "build": "vite build",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:e2e": "playwright test",
    "lint:oxlint": "oxlint . --fix -D correctness --ignore-path .gitignore",
    "lint:eslint": "eslint . --fix",
    "lint": "run-s lint:*",
    "format": "prettier --write src/",
    "nginx": "docker run -p 80:80 -v $(pwd)/nginx:/etc/nginx -d nginx",
    "nginx:reload": "docker exec nginx nginx -s reload"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.2",
    "@vueuse/core": "^12.8.2",
    "element-plus": "^2.10.7",
    "localforage": "^1.10.0",
    "mitt": "^3.0.1",
    "monaco-editor": "^0.52.2",
    "pinia": "^3.0.3",
    "uuid": "^11.1.0",
    "vue": "^3.5.18",
    "vue-i18n": "^11.1.11",
    "vue-router": "^4.5.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@playwright/test": "^1.54.2",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vitejs/plugin-vue-jsx": "^4.2.0",
    "@vitest/eslint-plugin": "1.1.38",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/test-utils": "^2.4.6",
    "axios": "^1.11.0",
    "eslint": "^9.33.0",
    "eslint-plugin-oxlint": "^0.16.12",
    "eslint-plugin-playwright": "^2.2.2",
    "eslint-plugin-vue": "^10.4.0",
    "globals": "^16.3.0",
    "jsdom": "^26.1.0",
    "npm-run-all2": "^7.0.2",
    "oxlint": "^0.15.15",
    "prettier": "^3.6.2",
    "sass": "^1.90.0",
    "sass-loader": "^16.0.5",
    "unplugin-element-plus": "^0.9.1",
    "vite": "^6.3.5",
    "vite-plugin-monaco-editor": "^1.1.0",
    "vite-plugin-vue-devtools": "^7.7.7",
    "vitest": "^3.2.4"
  }
}
